<h1>Sign Up for MusicApp!</h1>

<!-- destination: also users_url should be in ruby tags -->
<form action="<%= users_url %>" method="post">
  <label for="email">Email: </label>
  <input id="email" type="text" name="user[email]" value="your email">
  <br>

  <label for="password">Password:</label>
  <input id="password" type="password" name="user[password]">
  <br>

  <input type="submit" value="Sign Up">
</form>
